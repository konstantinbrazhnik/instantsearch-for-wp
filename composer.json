{
    "name": "yoko-co/instantsearch-for-wp",
    "description": "InstantSearch for WP is a set of Gutenberg Blocks of InstantSearch.js widgets.",
    "require": {
        "woocommerce/action-scheduler": "^3.6",
        "appsero/client": "^2.0",
        "typesense/typesense-php": "^5.1",
        "algolia/algoliasearch-client-php": "^4.0",
        "symfony/http-client": "^6.4"
    },
    "autoload": {
        "psr-4": {
            "YokoCo\\": "includes/"
        },
		"files": [
			"includes/functions.php"
		]
    },
    "authors": [
        {
            "name": "Yoko Co",
            "email": "developer@yokoco.com"
        }
    ],
	"config": {
		"vendor-dir": "lib",
        "allow-plugins": {
            "php-http/discovery": true,
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
	},
    "require-dev": {
        "wp-coding-standards/wpcs": "^3.0",
        "squizlabs/php_codesniffer": "^3.13"
    }
}
